{
  "name": "@ng-config/schematics",
  "version": "0.0.1",
  "scripts": {
      "build": "tsc -p tsconfig.schematics.json",
      "copy:schemas": "cp --parents schematics/*/schema.json ../../../dist/ng-config/",
      "copy:files": "cp --parents -p schematics/*/files/** ../../../dist/ng-config/",
      "copy:collection": "cp schematics/collection.json ../../../dist/ng-config/schematics/collection.json",
      "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^8.0.0",
    "@angular-devkit/schematics": "^8.0.0",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "~3.4.3"
  }
}