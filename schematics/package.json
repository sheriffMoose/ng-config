{
  "name": "@ng-config/schematics",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../../dist/ng-config/",
    "copy:files": "cp --parents -r schematics/*/files/** ../../../dist/ng-config/",
    "copy:package": "cp schematics/package.json ../../../dist/ng-config/schematics/package.json",
    "copy:collection": "cp schematics/collection.json ../../../dist/ng-config/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection && npm run copy:package"
  },
  "schematics": "./schematics/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^8.0.0",
    "@angular-devkit/schematics": "^8.0.0",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "schematics-utilities": "^1.1.2",
    "typescript": "~3.4.3"
  }
}
